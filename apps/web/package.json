{
	"name": "web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"preview": "wrangler pages dev .svelte-kit/cloudflare",
		"deploy": "bun run build && wrangler pages deploy",
		"prepare": "ts-patch install && typia patch"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250204.0",
		"@hono/typia-validator": "^0.1.0",
		"@hono/vite-build": "^1.3.0",
		"@hono/vite-dev-server": "^0.18.1",
		"@repo/sveltekit-superforms": "workspace:*",
		"@ryoppippi/unplugin-typia": "^1.2.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-cloudflare": "^5.0.3",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.6",
		"@tanstack/svelte-query": "^5.66.0",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"daisyui": "^5.0.0-beta.7",
		"globals": "^15.0.0",
		"hono": "^4.7.0",
		"svelte": "^5.20.1",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.6",
		"ts-patch": "^3.3.0",
		"typescript": "^5.0.0",
		"typia": "^7.6.4",
		"vite": "^6.1.0",
		"wrangler": "^3.107.3"
	},
	"type": "module",
	"dependencies": {
		"appwrite": "^15.0.0",
		"node-appwrite": "^14.1.0"
	}
}